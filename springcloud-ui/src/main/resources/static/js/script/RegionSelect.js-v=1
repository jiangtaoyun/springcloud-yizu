  Lv1Checked: function (obj, Arraya, objId, isMulti, ShowLv3, doubleClickSave) {
    for (var i = 0; i < region1id.length; i++) {
      if (obj.id.substring(5) == region1id[i])
        Arraya = i;
    }
    this.rdsl2(Arraya, objId, isMulti, ShowLv3, doubleClickSave);
    if (document.getElementById("NavDiv")) {
      document.getElementById("NavDiv").style.display = "";
      document.getElementById("SelectedLi").style.display = "";
      document.getElementById("ProvinceDiv").style.display = "none";
    }
  },
  Lv1Move: function (obj) {
    var moveColor = "ffe9d3", borderColor = "";
    if (location.pathname.indexOf("/Company") == 0) {
      moveColor = "d3e9ff";
      borderColor = "";
    }
    if (obj.style.backgroundColor == '#f05502' || obj.style.backgroundColor == 'rgb(240, 85, 2)') {
      return;
    }
    if (obj.style.backgroundColor == '#' + moveColor || obj.style.backgroundColor == 'rgb(255, 233, 211)' || obj.style.backgroundColor == 'rgb(211, 233, 255)') {
      obj.style.backgroundColor = '';
    }
    else {
      obj.style.backgroundColor = '#' + moveColor;
    }
  },
  rdsl2: function (Arraya, objId, isMulti, ShowLv3, doubleClickSave) {
    BodyClass.hideSub();
    var subareaids = region2id[Arraya];
    var subareanames = region2[Arraya];
    var aId = region1id;
    var aNa = region1;
    if (subareaids == undefined) {
      if (isMulti == 0) {
        BodyClass.saveInt(aId[Arraya], aNa[Arraya], objId);
      }
      if (isMulti == 1) {
        if (aNa[Arraya].indexOf("香港") >= 0)
          BodyClass.bcb(document.getElementById("liReg90"), '' + objId + '', 'region2id', 'region2', Arraya)
        else if (aNa[Arraya].indexOf("澳门") >= 0)
          BodyClass.bcb(document.getElementById("liReg91"), '' + objId + '', 'region2id', 'region2', Arraya)
        else if (aNa[Arraya].indexOf("台湾") >= 0)
          BodyClass.bcb(document.getElementById("liReg92"), '' + objId + '', 'region2id', 'region2', Arraya)
        else if (aNa[Arraya].indexOf("国外") >= 0)
          BodyClass.bcb(document.getElementById("liReg93"), '' + objId + '', 'region2id', 'region2', Arraya)
      }
      return false;
    }
    var moveColor = "ffe9d3", borderColor = "";
    if (location.pathname.indexOf("/Company") == 0) {
      moveColor = "d3e9ff";
      borderColor = "";
    }
    if (ShowLv3 == 0 && (aNa[Arraya].indexOf("北京") >= 0 || aNa[Arraya].indexOf("天津") >= 0 || aNa[Arraya].indexOf("上海") >= 0 || aNa[Arraya].indexOf("重庆") >= 0) && objId!="RmListRegionID") {
      if (isMulti == 0) {
        BodyClass.saveInt(aId[Arraya], aNa[Arraya], objId);
      } else {
        _("showSelectedLi").innerHTML = "";
      }
      return false;
    }
    var iWidth = 50;
    var mLen = 2;
    for (var m = 0; m < subareaids.length; m++) {
      if (subareanames[m].length > mLen) mLen = subareanames[m].length;
    }
    charWidth = 16;
    if (mLen * charWidth > iWidth) {
      iWidth = mLen * (charWidth - 3);
    }
    if (isMulti > 0) {
      iWidth += 12;
    }
    if (aNa[Arraya].indexOf("重庆") > -1) {
      if (doubleClickSave == 2 || isMulti == 1)
        hDiv = '<ul style="height:200px;">';
      else
        hDiv = '<ul style="height:220px;">';
    }
    else {
      hDiv = '<ul style="height:115px;">';
    }
    hDiv = '<ul>';
    if (objId == "MapPlace" || doubleClickSave == 2 || isMulti == 1 || (doubleClickSave == 1 && (aId[Arraya] == "10" || aId[Arraya] == "30" || aId[Arraya] == "11" || aId[Arraya] == "60"))) {
      if (isMulti == 1)
        hDiv += '<li style="cursor:pointer; display:block; font-weight:bold ; float:none" id="Li' + aId[Arraya] + '" onMouseOver="this.style.backgroundColor=\'#' + moveColor + '\';" onMouseOut="this.style.backgroundColor=\'\';" ><span onclick="BodyClass.bcb(this, \'' + objId + '\', \'region1id\', \'region1\');"><input name="' + aId[Arraya] + '" value="' + aNa[Arraya] + '" type="checkbox" onclick="this.checked=!this.checked;BodyClass.hideSub()">' + aNa[Arraya] + '全部</span></li>';
      else
        hDiv += '<li style="cursor:pointer; display:block; font-weight:bold ; float:none" id="Li' + aId[Arraya] + '" onMouseOver="this.style.backgroundColor=\'#' + moveColor + '\';" onMouseOut="this.style.backgroundColor=\'\';" onClick="BodyClass.saveInt(\'' + aId[Arraya] + '\',\'' + aNa[Arraya] + '\',\'' + objId + '\');">' + aNa[Arraya] + '全部</li>';
    }
    for (var m = 0; m < subareaids.length; m++) {
      var strDblClick = "";
      if (ShowLv3 == 0) {
        var strOnClick = 'BodyClass.saveInt(\'' + subareaids[m] + '\',\'' + subareanames[m] + '\',\'' + objId + '\');';
      }
      else {
        var strOnClick = 'RegionSelect.rdsl3(event,this ,\'' + Arraya + '\',\'' + objId + '\',\'' + m + '\', ' + isMulti + ');';
        if (doubleClickSave > 0) {
          strDblClick = ' ondblclick="BodyClass.saveInt(\'' + subareaids[m] + '\',\'' + subareanames[m] + '\',\'' + objId + '\');"';
        }
      }
      if (isMulti == 0) {
        hDiv += '<li style="cursor:pointer;" id="Li' + subareaids[m] + '" onMouseOver="this.style.backgroundColor=\'#' + moveColor + '\';" onMouseOut="this.style.backgroundColor=\'\';" onClick="' + strOnClick + '"' + strDblClick + '>' + subareanames[m] + '<span class="popupDivArrowDown"></span></li>';
      }
      else {
        hDiv += '<li id="Li' + subareaids[m] + '" style="cursor:pointer;" onMouseOver="this.style.backgroundColor=\'#' + moveColor + '\';" onMouseOut="this.style.backgroundColor=\'\';"'
        if (ShowLv3 == 1) {
          hDiv += ' onClick="RegionSelect.rdsl3(event,this ,\'' + Arraya + '\',\'' + objId + '\',\'' + m + '\', ' + isMulti + ')"';
        }
        hDiv += '><span onClick="BodyClass.bcb(this, \'' + objId + '\', \'region2id\', \'region2\',' + Arraya + ');"><input name="' + subareaids[m] + '" value="' + subareanames[m] + '" type="checkbox" onclick="this.checked=!this.checked;" />';
        if (ShowLv3 == 1) {
          hDiv += '</span>' + subareanames[m];
        }
        else {
          hDiv += subareanames[m] + '</span>';
        }
        if (aId[Arraya].substr(0, 2) == "10" || aId[Arraya].substr(0, 2) == "11" || aId[Arraya].substr(0, 2) == "30" || aId[Arraya].substr(0, 2) == "60") {
          hDiv += '</li>';
        }
        else {
          hDiv += '<span class="popupDivArrowDown"></span></li>';
        }
      }

    }
    hDiv += '<div style="clear:both"></div>';
    hDiv += '</ul>';
    _("showSelectedLi").innerHTML = hDiv;
    var clientHeight = _("msg1").clientHeight;
    var styleTop = parseInt(_("msg1").style.top.substring(0, _("msg1").style.top.length - 2));
    var curScrollHeight = document.documentElement.scrollTop + document.body.scrollTop + document.documentElement.clientHeight;
    if (clientHeight + styleTop > curScrollHeight) {
      _("msg1").style.top = (curScrollHeight - clientHeight - 5) + 'px';
    }
    if (isMulti == 1) {
      BodyClass.changeChecked(objId);
    }
  },
  cancelEvent: function (e) {
    if (e && e.stopPropagation)
      e.stopPropagation();
    else
      window.event.cancelBubble = true;
    return false;
  },
  rdsl3: function (event, subId, Arraya, objId, Arrayb, isMulti) {
    BodyClass.hideSub();
    event = event ? event : window.event
    var tarObj = event.srcElement ? event.srcElement : event.target
    var _subId = subId.id.replace("Li", "");
    var aId = region3id;
    var aNa = region3;
    var _Divsub = _("popDivSub");
    var pos = subid2[Arraya][Arrayb]
    var subareaid = region3id[pos];
    var subarea = region3[pos];
    if (subareaid == undefined) {
      if (isMulti == 0) {
        BodyClass.saveInt(_subId, subId.innerHTML.toLowerCase().replace(/<span class=\"popupdivarrowdown\"><\/span>/g, '').replace(/<span class=popupdivarrowdown><\/span>/g, ''), objId);
      }
      return false;
    }		
    var moveColor = "ffe9d3", borderColor = "f44d09";
    if (location.pathname.indexOf("/Company") == 0) {
      moveColor = "d3e9ff";
      borderColor = "0095d7";
    }
    hDiv = '';
    //if (objId == "MapPlace" || isMulti==1  || RegionSelect.doubleClickSave>=1) {
    if (isMulti == 0) {
      if (RegionSelect.doubleClickSave >= 1) {
        hDiv += '<p style="cursor:pointer;width:83px;font-weight:bold;float:left" onMouseOver="this.style.backgroundColor=\'#' + moveColor + '\'" onMouseOut="this.style.backgroundColor=\'\'" onClick="BodyClass.saveInt(\'' + region2id[Arraya][Arrayb] + '\',\'' + region2[Arraya][Arrayb] + '\',\'' + objId + '\');">' + region2[Arraya][Arrayb] + (objId == "MapPlace" ? "全部" : "") + '<span class="popupDivArrowUp" onclick="BodyClass.hideSub();BodyClass.cancelEvent()"></span></p>';
      }
      else {
        hDiv += '<p style="cursor:pointer;width:83px;font-weight:bold;float:left" onMouseOver="this.style.backgroundColor=\'#' + moveColor + '\'" onMouseOut="this.style.backgroundColor=\'\'" onclick="BodyClass.hideSub();BodyClass.cancelEvent()">' + region2[Arraya][Arrayb] + (objId == "MapPlace" ? "全部" : "") + '<span class="popupDivArrowUp"></span></p>';
      }
    }
    else {
      hDiv += '<p style="cursor:pointer;width:83px;font-weight:bold;float:left" onMouseOver="this.style.backgroundColor=\'#' + moveColor + '\'" onMouseOut="this.style.backgroundColor=\'\'" onClick="BodyClass.bcb(this, \'' + objId + '\', \'region2id\', \'region2\',' + Arraya + ');" ><input name="' + region2id[Arraya][Arrayb] + '" value="' + region2[Arraya][Arrayb] + '" type="checkbox" onclick="this.checked=!this.checked;" />' + region2[Arraya][Arrayb] + '<span class="popupDivArrowUp" onclick="BodyClass.hideSub();BodyClass.cancelEvent()"></span></p>';
    }

    //}	

    hDiv += '<ul style="clear:both;width:178px;float:left">';

    for (var m = 0; m < subareaid.length; m++) {
      var strStyle = "";
      if (isMulti == 0) {
        hDiv += '<li style="cursor:pointer;width:85px;' + strStyle + '" onMouseOver="this.style.backgroundColor=\'#' + moveColor + '\'" onMouseOut="this.style.backgroundColor=\'#fff\'" onClick="BodyClass.saveInt(\'' + subareaid[m] + '\',\'' + subarea[m] + '\',\'' + objId + '\');">' + subarea[m] + '</li>';
      }
      else {
        hDiv += '<li style="cursor:pointer;width:85px;' + strStyle + '" onMouseOver="this.style.backgroundColor=\'#' + moveColor + '\';" onMouseOut="this.style.backgroundColor=\'#fff\';" onClick="BodyClass.bcb(this, \'' + objId + '\', \'region3id\', \'region3\',' + Arraya + ');"><input name="' + subareaid[m] + '" value="' + subarea[m] + '" type="checkbox" onclick="this.checked=!this.checked;" />' + subarea[m] + '</li>';
      }
    }
    hDiv += '<div style="clear:both"></div>';
    hDiv += '</ul>';
    _Divsub.innerHTML = hDiv;
    var _DivsubTop = BodyClass.gaot(subId) - BodyClass.gaot(_("msg1"));
    var _DivsubLeft = BodyClass.gaol(subId) - BodyClass.gaol(_("msg1"));
    if (isMulti == 1) {
      BodyClass.scb(objId, 'region3id', 'region3');
    }
    var parentObj = tarObj;
    var parentOffTop = tarObj.offsetTop;
    while (parentObj = parentObj.offsetParent) {
      parentOffTop += parentObj.offsetTop;
    }
    _DivsubTop = _DivsubTop + 2;
    _Divsub.style.top = _DivsubTop + "px";
    _Divsub.style.left = _DivsubLeft + "px";
    BodyClass.showSub();
    var arrSubLi = _Divsub.getElementsByTagName("li");
    if ((this.GetLeft(_Divsub) + _Divsub.offsetWidth) > (this.GetLeft(_("msg1")) + _("msg1").offsetWidth)) {
      var arrSubP = _("popDivSub").getElementsByTagName("p");
      if (arrSubP.length > 0) {
        arrSubP[0].style.marginLeft = "87px"
        arrSubP[0].style.paddingRight = "4px"
      }
      arrSubLi[1].style.borderTop = "none";
      arrSubLi[1].style.height = "20px"
      _Divsub.style.left = _DivsubLeft - 89 + "px";
    }
    if (isMulti == 1) {
      BodyClass.changeChecked(objId);
    }
    return;
  },
  GetLeft: function (obj) {
    var l = obj.offsetLeft;
    while (obj = obj.offsetParent) {
      l += obj.offsetLeft;
    }
    return l;
  },
  ReturnP: function () {
    document.getElementById("NavDiv").style.display = "none";
    document.getElementById("SelectedLi").style.display = "none";
    document.getElementById("ProvinceDiv").style.display = "";
  }
  , doubleClickSave: 0
}