function TriggleDetail(obj, row) {
    /* $('#trDetail' + row).toggle();
    if ($('#trDetail' + row).css("display")!="none") {
    addViewLog(row);
    $(obj).attr("class", "PointerUp");
    }
    else {
    $(obj).attr("class", "PointerDown");
    }*/
    if ($('#trDetail' + row).css("display") == "none") {
        addViewLog(row);
        $(obj).attr("class", "PointerUp");
        $('#trDetail' + row).css("display", "table-row");
    }
    else {
        $(obj).attr("class", "PointerDown");
        $('#trDetail' + row).css("display", "none");
    }
}

function addViewLog(row) {
    if ($("#JobID" + row).length > 0 && hasInsertViewLog == 0) {
        var id = $("#JobID" + row).val();
        $.post("/Personal/js/AjaxAddJobViewLog?radom=" + new Date(), {
            JobIDs: id
        });
    }
}

function IsChildJobType(parent, id) {
    if ((parent == 46 && id == 1116) || (parent == 38 && id == 1117) || (parent == 33 && id == 1118) || (parent == 31 && id == 1119) || (parent == 51 && id == 1120) || (parent == 12 && id == 1322) || (parent == 55 && id == 1617) || (parent == 39 && id == 1621) || (parent == 40 && id == 1821) || (parent == 43 && id == 1918) || (parent == 49 && id == 2211) || (parent == 21 && id == 2218) || (parent == 49 && id == 2219) || (parent == 24 && id == 2713) || (parent == 38 && id == 2723) || (parent == 49 && id == 3224) || (parent == 49 && id == 4010) || (parent == 56 && id == 4516) || (parent == 50 && id == 4713) || (parent == 49 && id == 4818) || (parent == 22 && id == 4910) || (parent == 50 && id == 4915) || (parent == 49 && id == 5014) || (parent == 55 && id == 5113)) {
        return true;
    }
    else
        return false;
}

function FilterAll(obj) {
    if (obj.className == "FilterAllMore") {
        document.getElementById("FilterMainTable").style.display = "block"
        obj.className = "FilterAllLess";
    } else {
        document.getElementById("FilterMainTable").style.display = "none"
        obj.className = "FilterAllMore";
    }

}

function ChangeViewFilter() {
    var trHidden = getElementsByName("tr", "trHidden");
    if (trHidden.length > 0) {
        var tableHidden = trHidden[0].parentNode;
        var ChildNodes = tableHidden.getElementsByTagName("tr").length;
        for (var i = 0; i < trHidden.length; i++) {
            if (trHidden[i]) {
                if (trHidden[i].style.display == "none") {
                    trHidden[i].style.display = "table-row";
                } else {
                    if (i >= 3 - ChildNodes + trHidden.length)
                        trHidden[i].style.display = "none";
                }
            }
        }
    }
    var ViewPointer = document.getElementById("ViewPointer");
    if (ViewPointer.className == "ViewPointerDown") {
        ViewPointer.className = "ViewPointerUp";
    } else {
        ViewPointer.className = "ViewPointerDown";
    }
}

function NormalTrHidden() {
    var tableTemp = document.getElementById('FilterMainTable');
    if (!tableTemp) {
        return;
    }
    if (tableTemp.getElementsByTagName("tr").length <= 3) {
        var ViewPointer = document.getElementById("ViewPointer");
        ViewPointer.parentNode.style.display = "none";

    } else {
        var ViewPointer = document.getElementById("ViewPointer");
        ViewPointer.parentNode.style.display = "block";
    }
    var trHidden = getElementsByName("tr", "trHidden");
    if (trHidden.length > 0) {
        var tableHidden = trHidden[0].parentNode;
        var ChildNodes = tableHidden.getElementsByTagName("tr").length;
        for (var i = 0; i < 3 - ChildNodes + trHidden.length; i++) {
            if (trHidden[i]) {
                trHidden[i].style.display = "table-row";
            }
        }
    }

}

// 兼容IE FF的ByName方法
var getElementsByName = function (tag, name) {
    var returns = document.getElementsByName(name);
    if (returns.length > 0) return returns;
    returns = new Array();
    var e = document.getElementsByTagName(tag);
    for (var i = 0; i < e.length; i++) {
        if (e[i].getAttribute("name") == name) {
            returns[returns.length] = e[i];
        }
    }
    return returns;
}

//搜索关键词增加搜索企业
function showSearchCp(num) {
    if ($(".searchCp").is(":visible")) {
        $(".showCpListSpan").html(num +"个其他相关公司");
        $(".searchCp").hide();
    } else {
        $(".showCpListSpan").html("收起");
        $(".searchCp").show();
    }
}

