function BreifView() {
    hasInsertViewLog = 1
    $("[class='PointerDown']").each(function () {
        $(this).click();
    });
    $("#divExpand").attr("class", "ViewByContent");
    $("#hidExpand").val("1");
}

function ListView() {
    hasInsertViewLog = 0;
    $("[class='PointerUp']").each(function () {
        $(this).click();
    });

    $("#divExpand").attr("class", "ViewByList");
    $("#hidExpand").val("0");
}

